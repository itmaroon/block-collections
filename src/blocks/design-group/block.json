{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "itmar/design-group",
	"version": "0.1.0",
	"title": "Design Group",
	"category": "design",
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": false,
			"gradients": true,
			"custom": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"style": true,
				"width": true
			}
		}
	},
	"attributes": {
		"positionType": {
			"type": "string",
			"default": "relative"
		},
		"heightValue": {
			"type": "string",
			"default": "fit"
		},
		"default_pos": {
			"type": "object",
			"default": {
				"direction": "horizen",
				"inner_align": "flex-start",
				"outer_align": "center",
				"outer_vertical": "center",
				"width_val": "wideSize",
				"free_val": "400px",
				"posValue": {
					"vertBase": "top",
					"horBase": "left",
					"vertValue": "3em",
					"horValue": "3em"
				},
				"margin": {
					"top": "0px",
					"left": "0px",
					"bottom": "0px",
					"right": "0px"
				},
				"padding": {
					"top": "0px",
					"left": "0px",
					"bottom": "0px",
					"right": "0px"
				},
				"padding_content": {
					"top": "0px",
					"left": "0px",
					"bottom": "0px",
					"right": "0px"
				},
				"grid_info": {
					"gridElms": [],
					"rowNum": 2,
					"colNum": 2,
					"rowGap": "5px",
					"colGap": "5px",
					"rowUnit": [],
					"colUnit": []
				}
			}
		},
		"mobile_pos": {
			"type": "object",
			"default": {
				"direction": "vertical",
				"inner_align": "flex-start",
				"outer_align": "center",
				"outer_vertical": "center",
				"width_val": "wideSize",
				"free_val": "200px",
				"posValue": {
					"vertBase": "top",
					"horBase": "left",
					"vertValue": "2em",
					"horValue": "1em"
				},
				"margin": {
					"top": "0px",
					"left": "0px",
					"bottom": "0px",
					"right": "0px"
				},
				"padding": {
					"top": "0px",
					"left": "0px",
					"bottom": "0px",
					"right": "0px"
				},
				"padding_content": {
					"top": "20px",
					"left": "10px",
					"bottom": "20px",
					"right": "10px"
				},
				"grid_info": {
					"gridElms": [],
					"rowNum": 2,
					"colNum": 2,
					"rowGap": "5px",
					"colGap": "5px"
				}
			}
		},
		"shadow_element": {
			"type": "object",
			"default": {
				"shadowType": "nomal",
				"spread": 2,
				"lateral": 2,
				"longitude": 2,
				"nomalBlur": 3,
				"shadowColor": "#9F9F9F",
				"distance": 5,
				"intensity": 5,
				"opacity": 0.5,
				"depth": 5,
				"blur": 5,
				"bdBlur": 5,
				"expand": 5,
				"glassblur": 5,
				"glassopa": 0.5,
				"newDirection": "top_left",
				"clayDirection": "top",
				"embos": "swell",
				"hasOutline": true,
				"baseColor": "#ffffff"
			}
		},
		"shadow_result": {
			"type": "object"
		},
		"is_shadow": {
			"type": "boolean",
			"default": false
		},
		"is_anime": {
			"type": "boolean",
			"default": false
		},
		"anime_prm": {
			"type": "object",
			"default": {
				"pattern": "fadeLeft",
				"trigger": "opend",
				"delay": 0,
				"duration": 0.5
			}
		},
		"blockNum": {
			"type": "number",
			"default": 1
		},
		"is_moveable": {
			"type": "boolean",
			"default": false
		},
		"position": {
			"type": "object",
			"default": {
				"x": "0px",
				"y": "0px"
			}
		},
		"is_menu": {
			"type": "boolean",
			"default": false
		},
		"is_swiper": {
			"type": "boolean",
			"default": false
		},
		"parallax_obj": {
			"type": "object",
			"default": null
		},
		"is_submenu": {
			"type": "boolean",
			"default": false
		},
		"has_submenu": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "block-collections",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}