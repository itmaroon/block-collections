{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "itmar/design-process",
	"version": "0.1.0",
	"title": "Design Process",
	"category": "design",
	"supports": {
		"html": false
	},
	"attributes": {
		"figure_blocks": {
			"type": "array",
			"default": []
		},
		"bgColor_form": {
			"type": "string"
		},
		"bgGradient_form": {
			"type": "string"
		},
		"radius_form": {
			"type": "object",
			"default": {
				"topLeft": "0px",
				"topRight": "0px",
				"bottomRight": "0px",
				"bottomLeft": "0px",
				"value": "0px"
			}
		},
		"border_form": {
			"type": "object"
		},
		"margin_form": {
			"type": "object",
			"default": {
				"top": "1em",
				"left": "2em",
				"bottom": "1em",
				"right": "2em"
			}
		},
		"padding_form": {
			"type": "object",
			"default": {
				"top": "1em",
				"left": "2em",
				"bottom": "1em",
				"right": "2em"
			}
		},
		"font_style_num": {
			"type": "object",
			"default": {
				"fontSize": "16px",
				"fontFamily": "Noto Sans JP, sans-serif",
				"fontWeight": "500",
				"isItalic": false
			}
		},
		"textColor_num": {
			"type": "string",
			"default": "#000"
		},
		"bgColor_num": {
			"type": "string",
			"default": "#f08f8f"
		},
		"font_style_process": {
			"type": "object",
			"default": {
				"fontSize": "16px",
				"fontFamily": "Noto Sans JP, sans-serif",
				"fontWeight": "500",
				"isItalic": false
			}
		},
		"textColor_process": {
			"type": "string",
			"default": "#000"
		}
	},
	"usesContext": [
		"itmar/state_process"
	],
	"textdomain": "design-process",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}