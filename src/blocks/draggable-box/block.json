{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "itmar/draggable-box",
	"version": "1.0.0",
	"title": "Draggable Box",
	"category": "widgets",
	"icon": "tickets",
	"supports": {
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"direction": {
			"type": "string",
			"default": "block"
		},
		"inner_align": {
			"type": "string",
			"default": "flex-start"
		},
		"outer_align": {
			"type": "string",
			"default": "center"
		},
		"width_val": {
			"type": "string",
			"default": "wideSize"
		},
		"is_mobile_vertical": {
			"type": "boolean",
			"default": false
		},
		"is_moveable": {
			"type": "boolean",
			"default": false
		},
		"position": {
			"type": "object",
			"default": {
				"x": 0,
				"y": 0
			}
		},
		"unit_x": {
			"type": "string",
			"default": "px"
		},
		"unit_y": {
			"type": "string",
			"default": "px"
		}
	},
	"textdomain": "itmar_block_collections",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}