{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "itmar/swiper-masonry",
  "version": "0.1.0",
  "title": "Swiper Masonry",
  "category": "widgets",
  "icon": "schedule",
  "description": "マソンリー形式で配置して、クリックしたらスライダー表示させるブロックです。",
  "supports": {
    "html": false
  },
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p"
    }
  },
  "textdomain": "block-location",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}