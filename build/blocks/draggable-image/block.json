{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "itmar/draggable-image",
  "version": "0.1.0",
  "title": "Draggable Image",
  "category": "widgets",
  "icon": "tickets",
  "description": "ドラッグできるcore/imageブロックです",
  "supports": {
    "html": false
  },
  "attributes": {
    "position": {
      "type": "object",
      "default": {
        "x": 0,
        "y": 0
      }
    }
  },
  "textdomain": "draggable-image",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}