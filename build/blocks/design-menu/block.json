{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "itmar/design-menu",
  "version": "0.1.0",
  "title": "Design Menu",
  "category": "design",
  "supports": {
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "color": {
      "background": true,
      "text": false,
      "custom": true
    }
  },
  "attributes": {
    "inner_align": {
      "type": "string",
      "default": "flex-start"
    },
    "outer_align": {
      "type": "string",
      "default": "center"
    },
    "outer_vertical": {
      "type": "string"
    },
    "width_val": {
      "type": "string",
      "default": "wideSize"
    },
    "free_val": {
      "type": "number"
    },
    "radius_val": {
      "type": "object",
      "default": {
        "topLeft": "0px",
        "topRight": "0px",
        "bottomRight": "0px",
        "bottomLeft": "0px",
        "value": "0px"
      }
    },
    "border_val": {
      "type": "object"
    },
    "shadow_element": {
      "type": "object",
      "default": {
        "shadowType": "nomal",
        "spread": 2,
        "lateral": 2,
        "longitude": 2,
        "nomalBlur": 3,
        "shadowColor": "#9F9F9F",
        "distance": 5,
        "intensity": 5,
        "opacity": 0.5,
        "depth": 5,
        "blur": 5,
        "bdBlur": 5,
        "expand": 5,
        "glassblur": 5,
        "glassopa": 0.5,
        "newDirection": "top_left",
        "clayDirection": "top",
        "embos": "swell",
        "hasOutline": true,
        "baseColor": "#ffffff"
      }
    },
    "shadow_result": {
      "type": "object"
    },
    "is_shadow": {
      "type": "boolean",
      "default": false
    },
    "grid_info": {
      "type": "object",
      "default": {
        "col_num": 2,
        "is_image": false,
        "image_pos": "top left",
        "image_padding": {
          "top": "10px",
          "left": "10px",
          "bottom": "10px",
          "right": "10px"
        },
        "image_radius": {
          "topLeft": "0px",
          "topRight": "0px",
          "bottomRight": "0px",
          "bottomLeft": "0px",
          "value": "0px"
        },
        "image_blur": 0
      }
    },
    "blockNum": {
      "type": "number",
      "default": 1
    },
    "is_submenu": {
      "type": "boolean",
      "default": false
    },
    "is_moveable": {
      "type": "boolean",
      "default": false
    },
    "position": {
      "type": "object",
      "default": {
        "x": 0,
        "y": 0
      }
    },
    "unit_x": {
      "type": "string",
      "default": "px"
    },
    "unit_y": {
      "type": "string",
      "default": "px"
    }
  },
  "textdomain": "itmar_block_collections",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}