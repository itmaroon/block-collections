{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "itmar/dragg-test",
  "version": "0.1.0",
  "title": "Dragg Test",
  "category": "widgets",
  "icon": "randomize",
  "description": "ドラッグのテストです",
  "supports": {
    "html": false
  },
  "attributes": {
    "scale": {
      "type": "object",
      "default": {
        "width": "100px",
        "height": "100px"
      }
    },
    "position": {
      "type": "object",
      "default": {
        "transform": "translate3D(0, 0, 0)"
      }
    }
  },
  "textdomain": "dragg-test",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}