{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "itmar/design-button",
  "version": "0.1.0",
  "title": "Design Button",
  "category": "widgets",
  "icon": "button",
  "description": "いくつかのデザインを集めたボタンです。",
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "newmor",
      "label": "Neumorphism"
    }
  ],
  "example": {},
  "attributes": {
    "btnalign": {
      "type": "string",
      "default": "none"
    },
    "btnContent": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "backgroundColor": {
      "type": "string"
    },
    "backgroundgradient": {
      "type": "string"
    },
    "btnTextColor": {
      "type": "string"
    },
    "btnbackgroundColor": {
      "type": "string"
    },
    "btngradient": {
      "type": "string"
    },
    "borderRadiusValues": {
      "type": "object",
      "default": {
        "value": "0px"
      }
    },
    "marginValues": {
      "type": "object",
      "default": {
        "top": "0px",
        "left": "0px",
        "right": "0px",
        "bottom": "0px"
      }
    },
    "paddingValues": {
      "type": "object",
      "default": {
        "top": "0px",
        "left": "0px",
        "right": "0px",
        "bottom": "0px"
      }
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "design-button",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}